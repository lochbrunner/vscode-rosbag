{
  "name": "vscode-rosbag",
  "version": "0.0.3",
  "displayName": "Rosbag Preview",
  "description": "Preview of Rosbag for VSCode",
  "main": "./dist/extension",
  "publisher": "lochbrunner",
  "repository": {
    "type": "git",
    "url": "https://github.com/lochbrunner/vscode-rosbag.git"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "keywords": [
    "rosbag",
    "vscode-extension"
  ],
  "author": "Matthias Lochbrunner",
  "license": "MIT",
  "activationEvents": [
    "onLanguage:rosbag",
    "onCommand:rosbag.preview"
  ],
  "engines": {
    "vscode": "^1.25.0"
  },
  "contributes": {
    "languages": [{
      "id": "rosbag",
      "extensions": [
        ".bag"
      ],
      "aliases": [
        "Rosbag"
      ],
      "configuration": "./language-configuration.json"
    }],
    "commands": [{
      "command": "rosbag.preview",
      "title": "Show Rosbag content",
      "category": "rosbag"
    }, {
      "command": "rosbag.info",
      "title": "Show Rosbag info",
      "category": "rosbag"
    }],
    "menus": {
      "explorer/context": [{
        "command": "rosbag.preview",
        "when": "resourceLangId == rosbag",
        "group": "rosbag"
      }, {
        "command": "rosbag.info",
        "when": "resourceLangId == rosbag",
        "group": "rosbag"
      }]
    },
    "configuration": {
      "type": "object",
      "title": "Rosbag Preview configuration",
      "properties": {
        "rosbag.previewCmdPath": {
          "type": ["string", "null"],
          "default": null,
          "description": "Path to an alternative command which provides preview information of the rosbag."
        }
      }
    }
  },
  "devDependencies": {
    "@types/node": "^11.9.5",
    "typescript": "^3.3.3333",
    "vscode": "^1.1.30"
  }
}
